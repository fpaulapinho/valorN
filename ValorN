import java.util.Scanner; 

public class Main {
 
    public static void main(String[] args) {
 
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for (int i = 2; i <= n; i+=2) {
            System.out.printf("%d^2 = %.0f\n", i, Math.pow(i, 2.0));
        }
 
    }
 
}
